- var helper = App.ViewHelpers.<%= controller_name.downcase %>;
%div#app_sub_header
  %a(href='#new' class=helper.links.new)Add New <%= controller_name.singularize %>
  %div(class=helper.css.new)
%ul(class=App.ViewHelpers.<%= controller_name.downcase %>.css.list)
  - $.each(collection.models, function(index, val) {
  - var <%= controller_name.singularize.downcase %> = val;
  - var htmlId = helper.id.tag(<%= controller_name.singularize.downcase %>.id);
  - var cycle = index % 2 ? 'even' : 'odd'
  %li.<%= controller_name.singularize.downcase %>(id=htmlId class=cycle)
  - });
